CREATE TABLE `miconnect`.`users` ( `id` INT NOT NULL AUTO_INCREMENT , `user_name` VARCHAR(255) NOT NULL , `full_name` VARCHAR(255) NOT NULL , `email` VARCHAR(255) NOT NULL , `contact` VARCHAR(255) NOT NULL , `password` VARCHAR(255) NOT NULL , `gender` VARCHAR(50) NOT NULL , `birth_date` VARCHAR(255) NOT NULL , `image` VARCHAR(255) NOT NULL , `created_at` TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP , PRIMARY KEY (`id`)) ENGINE = InnoDB;